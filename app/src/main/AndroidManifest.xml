<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.communityapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".wall.Wall"/>
        <activity android:name=".register.Register_OTP" />
        <activity
            android:name=".register.Register_password"
            android:parentActivityName=".register.Register_MobileNo" />
        <activity
            android:name=".register.Register_MobileNo"
            android:parentActivityName=".register.Register_DOB" />
        <activity
            android:name=".register.Register_DOB"
            android:parentActivityName=".register.Register_name" />
        <activity android:name=".register.Register_name" />
        <activity android:name=".login.reset_password" />
        <activity android:name=".login.otp_screen" />
        <activity android:name=".login.enter_number" />
        <activity android:name=".login.confirm" />
        <activity android:name=".login.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>